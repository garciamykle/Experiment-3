#include <iostream>

using namespace std;

int main()
{

	char i, j, temp, arr[100];
	int n;
	
		cout << "Enter the elements to be sorted: ";
		cin >> n;
		for(i=0;i<n;i++)
	{
		cin >> arr[i];
	}
	cout << endl << endl;
	
	for(i=0;i<n;i++)
	{
		for (j=0;j<n-i-1;j++)
		{
			if (arr[j] > arr[j+1])
			{
				temp=arr[j];
				arr[j]=arr[j+1];
				arr[j+1]=temp;	
			}
			
		}
	}
	cout << "The array elements after sorting is: ";
	for (i=0;i<n;i++)
	{
		cout << arr[i] << ", ";
	}
	return 0;
}
